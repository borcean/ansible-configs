---
# base
# ansible setup
branch: dev

timers:
   refresh:
      timer_boot: 1s
      timer_active: 1h
   pull:
      timer_boot: 10s
      timer_active: 10m

# users
user_ana: true
user_josh: true
user_nelu: true

# system setup
fail2ban: true

# workstation
# optional packages
toolbox_install: true
virt_manager_install: true